@model CollectionApp.Models.City

@{
    ViewBag.Title = "Details";
}

<div>
    <h4>Город</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Наименивание:

        <dd>
            @Html.DisplayFor(model => model.Name)
        </dd>
        <br />
        <dt>
            Создан:
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Creation)
        </dd>
        <br />
        <dt>
            Изменил:
        </dt>

        <dd>
            @Html.DisplayFor(model => model.LastUpdateUser)
        </dd>
        <br />
        <dt>
            Последнее обновление:
        </dt>

        <dd>
            @Html.DisplayFor(model => model.LastUpdate)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Изменить", "Edit", new { id = Model.Id }) |
    @Html.ActionLink("К списку", "Index")
</p>
